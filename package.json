{
  "name": "@mondrian-framework/root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "build-docs": "npm run build-docs --workspace=@mondrian-framework/docs",
    "test": "npm run test --workspaces --if-present",
    "pretty": "prettier --write \"./**/*.ts\"",
    "example": "npm run build --workspace=@mondrian-framework/example && npm run example --workspace=@mondrian-framework/example"
  },
  "devDependencies": {
    "typescript": "5.0.4",
    "vitest": "0.31.0",
    "prettier": "2.8.8"
  },
  "workspaces": [
    "packages/docs",
    "packages/utils",
    "packages/model",
    "packages/module",
    "packages/prisma",
    "packages/rest",
    "packages/graphql",
    "packages/aws-sqs",
    "packages/aws-lambda-sqs",
    "packages/cron",
    "packages/example"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {}
}
